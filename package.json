{
  "name": "baseapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "npm run build",
    "lint:check": "eslint . --ext=js,jsx",
    "lint:fix": "eslint . --ext=js,jsx --fix",
    "node_modules": "rm -rf node_modules/ && rm -rf package-lock.json",
    "node_modules-w": "rmdir node_modules /s /q && del package-lock.json",
    "install:clean": "npm run node_modules && npm install && npm start",
    "install:clean-w": "npm run node_modules-w && npm install && npm start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "3.9.1",
    "@material-ui/icons": "3.0.2",
    "chartist": "0.11.0",
    "history": "4.7.2",
    "i18next": "^14.0.1",
    "i18next-browser-languagedetector": "^2.2.4",
    "i18next-xhr-backend": "^1.5.1",
    "json-api-serializer": "^1.14.0",
    "jwt-decode": "^2.2.0",
    "leaflet": "1.4.0",
    "moment": "2.24.0",
    "node-sass-chokidar": "1.3.4",
    "nouislider": "12.1.0",
    "npm-run-all": "4.1.5",
    "perfect-scrollbar": "1.4.0",
    "react": "16.7.0",
    "react-big-calendar": "0.20.3",
    "react-bootstrap-sweetalert": "4.4.1",
    "react-chartist": "0.13.1",
    "react-check-auth": "^0.2.0-alpha.2",
    "react-context-global-state": "^2.1.5",
    "react-datetime": "2.16.3",
    "react-dom": "16.7.0",
    "react-google-maps": "9.4.5",
    "react-i18next": "^9.0.9",
    "react-jvectormap": "0.0.6",
    "react-leaflet": "^2.2.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.3",
    "react-swipeable-views": "0.13.1",
    "react-table": "6.8.6",
    "react-tagsinput": "3.19.0"
  },
  "optionalDependencies": {
    "@babel/core": "7.2.2",
    "@types/googlemaps": "3.30.16",
    "@types/markerclustererplus": "2.1.33",
    "ajv": "6.7.0",
    "babel-eslint": "9.0.0",
    "eslint": "5.6.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "prettier": "1.16.1"
  }
}
